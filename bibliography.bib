	month = {oct},
	organization = {IEEE},
	title = {{The Retail Store as a Smart Grid Ready Building: Current Practice and Future Potentials}},
	year = {2018},
	doi={10.1109/ISGT.2018.8403354},
    pubstate={Published}
}

